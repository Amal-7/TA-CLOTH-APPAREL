<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script>
  google.charts.load('current', { 'packages': ['corechart'] });
  google.charts.setOnLoadCallback(drawChart);

  function drawChart() {

    var data = google.visualization.arrayToDataTable([
      ['Task', 'Hours per Day'],
    <% if(payMethod.COD) { %>
        ['COD',  <%= payMethod.COD %>]  ,
      <% } %>


     <% if(payMethod.paypal) { %>
         ['PAYPAL',  <%= payMethod.paypal %>],
      <% } %>

      <% if(payMethod.razorpay) { %>
       ['RAZORPAY',  <%= payMethod.razorpay %>],
      <% } %>


    ]);

    var options = {
      title: 'total sales'
    };

    var chart = new google.visualization.PieChart(document.getElementById('piechart'));

    chart.draw(data, options);
  }

</script>

<script>
    document.addEventListener("DOMContentLoaded", () => {
      echarts.init(document.querySelector("#trafficChart")).setOption({
        tooltip: {
          trigger: 'item'
        },
        legend: {
          top: '5%',
          left: 'center'
        },
        series: [{
          name: "",
          type: 'pie',
          radius: ['40%', '70%'],
          avoidLabelOverlap: false,
          label: {
            show: false,
            position: 'center'
          },
          emphasis: {
            label: {
              show: true,
              fontSize: '18',
              fontWeight: 'bold'
            }
          },
          labelLine: {
            show: false
          },
          data: [

          <% if(orderStatus.pending) { %>
           
                {
            
                    value: <%= locals.orderStatus.pending %>,
                    name: 'pending'
           
              
              },

              <% } %>
           
          {
            value: <%= locals.orderStatus.placed %>,
            name: 'placed'
          },
          {
            value: <%= locals.orderStatus.Returned %>,
            name: 'Returned'
          },
          
          {
            value: <%= locals.orderStatus.Cancelled %>,
            name: 'cancel'
          },
          {
            <% if(orderStatus.shipped) { %>
            value: <%= locals.orderStatus.shipped %>,
            name: 'shipped'
            <% } %>
          },
          {
            value: <%= locals.orderStatus.Delivered %>,
            name: 'delivered'
          },

          ]
        }]
      });
    });

  </script>

